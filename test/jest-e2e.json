{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "aggregator /cache-manager/(.*)": "<rootDir>/../libs/cache-manager/src/$1",
    "aggregator /cache-manager": "<rootDir>/../libs/cache-manager/src",
    "aggregator/unique-id-generator/(.*)": "<rootDir>/../libs/unique-id-generator/src/$1",
    "aggregator/unique-id-generator": "<rootDir>/../libs/unique-id-generator/src",
    "@aggregator/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@aggregator/logger": "<rootDir>/../libs/logger/src",
    "@aggregator/db-manager/(.*)": "<rootDir>/../libs/db-manager/src/$1",
    "@aggregator/db-manager": "<rootDir>/../libs/db-manager/src",
    "@aggregator/common-types/(.*)": "<rootDir>/../libs/common-types/src/$1",
    "@aggregator/common-types": "<rootDir>/../libs/common-types/src",
    "@aggregator/sqs-manager/(.*)": "<rootDir>/../libs/sqs-manager/src/$1",
    "@aggregator/sqs-manager": "<rootDir>/../libs/sqs-manager/src",
    "@aggregator/sns-manager/(.*)": "<rootDir>/../libs/sns-manager/src/$1",
    "@aggregator/sns-manager": "<rootDir>/../libs/sns-manager/src"
  }
}
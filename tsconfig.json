{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "resolveJsonModule": true,
    "paths": {
      "@aggregator/cache-manager": [
        "libs/cache-manager/src"
      ],
      "@aggregator/cache-manager/*": [
        "libs/cache-manager/src/*"
      ],
      "@aggregator/unique-id-generator": [
        "libs/unique-id-generator/src"
      ],
      "@aggregator/unique-id-generator/*": [
        "libs/unique-id-generator/src/*"
      ],
      "@aggregator/logger": [
        "libs/logger/src"
      ],
      "@aggregator/logger/*": [
        "libs/logger/src/*"
      ],
      "@aggregator/db-manager": [
        "libs/db-manager/src"
      ],
      "@aggregator/db-manager/*": [
        "libs/db-manager/src/*"
      ],
      "@aggregator/common-types": [
        "libs/common-types/src"
      ],
      "@aggregator/common-types/*": [
        "libs/common-types/src/*"
      ],
      "@aggregator/sqs-manager": [
        "libs/sqs-manager/src"
      ],
      "@aggregator/sqs-manager/*": [
        "libs/sqs-manager/src/*"
      ],
      "@aggregator/sns-manager": [
        "libs/sns-manager/src"
      ],
      "@aggregator/sns-manager/*": [
        "libs/sns-manager/src/*"
      ],
      "@aggregator/events": [
        "libs/events/src"
      ],
      "@aggregator/events/*": [
        "libs/events/src/*"
      ]
    }
  }
}
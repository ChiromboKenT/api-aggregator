{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "monorepo": true,
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "request-handler": {
      "type": "application",
      "root": "apps/request-handler",
      "entryFile": "main",
      "sourceRoot": "apps/request-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/request-handler/tsconfig.app.json"
      }
    },
    "cache-manager": {
      "type": "library",
      "root": "libs/cache-manager",
      "entryFile": "index",
      "sourceRoot": "libs/cache-manager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache-manager/tsconfig.lib.json"
      }
    },
    "unique-id-generator": {
      "type": "library",
      "root": "libs/unique-id-generator",
      "entryFile": "index",
      "sourceRoot": "libs/unique-id-generator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/unique-id-generator/tsconfig.lib.json"
      }
    }
  }
}
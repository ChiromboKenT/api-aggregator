{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "monorepo": true,
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "request-handler": {
      "type": "application",
      "root": "apps/request-handler",
      "entryFile": "main",
      "sourceRoot": "apps/request-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/request-handler/tsconfig.app.json"
      }
    },
    "cache-manager": {
      "type": "library",
      "root": "libs/cache-manager",
      "entryFile": "index",
      "sourceRoot": "libs/cache-manager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache-manager/tsconfig.lib.json"
      }
    },
    "unique-id-generator": {
      "type": "library",
      "root": "libs/unique-id-generator",
      "entryFile": "index",
      "sourceRoot": "libs/unique-id-generator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/unique-id-generator/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "db-manager": {
      "type": "library",
      "root": "libs/db-manager",
      "entryFile": "index",
      "sourceRoot": "libs/db-manager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db-manager/tsconfig.lib.json"
      }
    },
    "common-types": {
      "type": "library",
      "root": "libs/common-types",
      "entryFile": "index",
      "sourceRoot": "libs/common-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-types/tsconfig.lib.json"
      }
    },
    "sqs-manager": {
      "type": "library",
      "root": "libs/sqs-manager",
      "entryFile": "index",
      "sourceRoot": "libs/sqs-manager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sqs-manager/tsconfig.lib.json"
      }
    },
    "sns-manager": {
      "type": "library",
      "root": "libs/sns-manager",
      "entryFile": "index",
      "sourceRoot": "libs/sns-manager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sns-manager/tsconfig.lib.json"
      }
    }
  }
}